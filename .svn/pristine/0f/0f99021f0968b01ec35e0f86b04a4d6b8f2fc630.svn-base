		return ac_endtime;
	}
	public void setAc_endtime(String ac_endtime) {
		this.ac_endtime = ac_endtime;
	}
	public int getAc_number() {
		return ac_number;
	}
	public void setAc_number(int ac_number) {
		this.ac_number = ac_number;
	}
