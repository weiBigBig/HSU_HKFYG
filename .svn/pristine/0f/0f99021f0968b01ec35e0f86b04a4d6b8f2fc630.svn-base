package com.djw.web.entity;

public class Activity {
	private String ac_id;
	private String ac_name;
	private String ac_company;
	private String ac_begintime;
	private String ac_endtime;
	private int ac_number;
	private int ac_hnumber; 
	private int ac_lnumber;
	private double ac_hour;
	private String ac_intro;
	public String getAc_id() {
		return ac_id;
	}
	public void setAc_id(String ac_id) {
		this.ac_id = ac_id;
	}
	public String getAc_name() {
		return ac_name;
	}
	public void setAc_name(String ac_name) {
		this.ac_name = ac_name;
	}
	public String getAc_company() {
		return ac_company;
	}
	public void setAc_company(String ac_company) {
		this.ac_company = ac_company;
	}
	public String getAc_begintime() {
		return ac_begintime;
	}
	public void setAc_begintime(String ac_begintime) {
		this.ac_begintime = ac_begintime;
	}
