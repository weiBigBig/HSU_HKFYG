		return ac_hnumber;
	}
	public void setAc_hnumber(int ac_hnumber) {
		this.ac_hnumber = ac_hnumber;
	}
	public int getAc_lnumber() {
		return ac_lnumber;
	}
	public void setAc_lnumber(int ac_lnumber) {
		this.ac_lnumber = ac_lnumber;
	}
	public double getAc_hour() {
		return ac_hour;
	}
	public void setAc_hour(double ac_hour) {
		this.ac_hour = ac_hour;
	}
	public String getAc_intro() {
		return ac_intro;
	}
	public void setAc_intro(String ac_intro) {
		this.ac_intro = ac_intro;
	}
}
